
// FERMI_TWOD_A
engine _2D 0x902D;

0x080 DestFormat;
0x081 DestIsLinear bool;
0x082 DestTileMode bits (
	0..3  Width;
	4..7  Height;
	8..11 Depth;
);
0x083 DestArrayMode;

0x086 DestHorizontal;
0x087 DestVertical;
0x088 DestAddr iova;

0x08c SrcFormat;
0x08d SrcIsLinear bool;
0x08e SrcTileMode bits (
	0..3  Width;
	4..7  Height;
	8..11 Depth;
);
0x08f SrcArrayMode;

0x092 SrcHorizontal;
0x093 SrcVertical;
0x094 SrcAddr iova;

0x0a4 ClipEnable bool;

0x0ab Operation enum (
	0 SrcCopyAnd;     // if (SRC.A) DST = SRC;
	1 RopAnd;         // R = ROP(DST, SRC, PATTERN); if (R.A) DST = R;
	2 Blend;          // DST = (1 - SRC.A * BETA1) * DST + SRC * SRC.A * BETA1;
	3 SrcCopy;        // DST = SRC;
	4 Rop;            // DST = ROP(DST, SRC, PATTERN);
	5 SrcCopyPremult; // DST = SRC * BETA4;
	6 BlendPremult;   // DST = (1 - SRC.A * BETA4.A) * DST + SRC * BETA4;
);

0x0b5 Unknown0b5;

0x221 Unknown221;

0x223 BlitControl bits (
	0 Origin enum (
		0 Center;
		1 Corner;
	);
	4 Filter enum (
		0 Nearest;
		1 Linear;
	);
);

0x22c BlitDestX;
0x22d BlitDestY;
0x22e BlitDestW;
0x22f BlitDestH;
0x230 BlitDuDxFract;
0x231 BlitDuDxInt;
0x232 BlitDvDyFract;
0x233 BlitDvDyInt;
0x234 BlitSrcXFract;
0x235 BlitSrcXInt;
0x236 BlitSrcYFract;
0x237 BlitSrcYInt;
